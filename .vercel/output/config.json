{
  "version": 3,
  "routes": [
    {
      "src": "^/manifest\\.webmanifest$",
      "headers": {
        "Content-Type": "application/manifest+json"
      },
      "continue": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    }
  ],
  "framework": {
    "version": "5.4.20"
  },
  "crons": []
}
